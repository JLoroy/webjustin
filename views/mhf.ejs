<body  ng-App="home" ng-controller="homeController">

<!-- Header -->
<div id="header">

    <div class="top">

        <!-- Logo -->
        <div id="logo">
            <h1 id="title">Justin Loroy</h1>
            <p>Computer Scientist</p>
            <span class="image avatar48"><img src="http://i.imgur.com/CYU8JsN.gif" alt="" /></span>
            <a style="width:50px;height:50px;" href="http://i.imgur.com/CYU8JsN.gif"></a>
        </div>
        <!-- Nav -->
        <nav id="nav">
            <!--

                Prologue's nav expects links in one of two formats:

                1. Hash link (scrolls to a different section within the page)

                   <li><a href="#foobar" id="foobar-link" class="icon fa-whatever-icon-you-want skel-layers-ignoreHref"><span class="label">Foobar</span></a></li>

                2. Standard link (sends the user to another page/site)

                   <li><a href="http://foobar.tld" id="foobar-link" class="icon fa-whatever-icon-you-want"><span class="label">Foobar</span></a></li>

            -->
            <ul>
                <li><a href="/#top" id="top-link" class="skel-layers-ignoreHref"><span class="icon fa-home">Intro</span></a></li>
                <li><a href="/#portfolio" id="portfolio-link" class="skel-layers-ignoreHref"><span class="icon fa-th">Portfolio</span></a></li>
                <li><a href="/#about" id="about-link" class="skel-layers-ignoreHref"><span class="icon fa-user">About Me</span></a></li>
                <li><a href="/#contact" id="contact-link" class="skel-layers-ignoreHref"><span class="icon fa-envelope">Contact</span></a></li>
            </ul>
            <ul>
                <li ng-if="permission>0"><a href="/blog"><span class="label">Blog</span></a></li>
                <li ng-if="permission>0"><a href="/nico"><span class="label">Nico</span></a></li>
                <li><a href="/armand"><span class="label">Armand Container</span></a></li>
                <li><a href="/hatonjeu" class="skel-layers-ignoreHref"><span class="label">Hatonjeu</span></a></li> <!--ng-click="goToPage('hatonjeu');"-->
                <li><a ng-if="permission>0"><a href="/avent"><span class="label">Calendrier de l'avent</span></a></li>
            </ul>
        </nav>

    </div>

    <div class="bottom">

        <!-- Social Icons -->
        <ul class="icons">
            <li><a href="https://twitter.com/JustinLoroy" class="icon fa-twitter"><span class="label">Twitter</span></a></li>
            <li><a href="https://www.facebook.com/justin.loroy" class="icon fa-facebook"><span class="label">Facebook</span></a></li>
            <!--<li><a href="https://github.com/JLoroy" class="icon fa-github"><span class="label">Github</span></a></li>-->
            <li><a href="mailto:justin@loroy.be" class="icon fa-envelope"><span class="label">Email</span></a></li>
            <li><a href="https://www.linkedin.com/in/justin-loroy-885a1462" class="icon fa-linkedin"><span class="label">LinkedIn</span></a></li>
        </ul>

    </div>

</div>

<!-- Main -->
<div id="main">
    <section id="top" class="one dark cover">
        <div class="container">
            <header>
                <h2 class="alt">My Human Farm</h2>
                <h3>Android minigame</h3>
            </header>
        </div>
    </section>

    <section style="background-color: black">
        <iframe  width="848" height="477"
                src="http://www.youtube.com/embed/oXuZHNrOLEU?autoplay=1">
        </iframe>
    </section>

</div>

