<html>
<head>
    <title>Calendrier de l'Avent</title>
    <meta charset="utf-8" />
    <script src="http://ajax.googleapis.com/ajax/libs/jquery/1/jquery.min.js"></script>
    <script src="http://ajax.googleapis.com/ajax/libs/jqueryui/1/jquery-ui.min.js"></script>
    <script src="/bootstrap/js/bootstrap.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.3.15/angular.js"></script>
    <script src="https://code.angularjs.org/1.3.15/i18n/angular-locale_fr-be.js"></script>
    <script src="javascripts/avent.js"></script>
    <link rel="stylesheet" href="stylesheets/avent.css">
</head>
<body  ng-App="avent" ng-controller="aventController">
<table class="table">
    <thead>    <th>Lundi</th>    <th>Mardi</th>    <th>Mercredi</th>    <th>Jeudi</th>    <th>Vendredi</th>    <th>Samedi</th>    <th>Dimanche</th>    </thead>
    <tbody>
        <tr ng-repeat="week in calendar">
            <td ng-repeat="day in week">
                <div class="day">
                    <h1>{{day.number}}</h1>
                    <p>{{day.secret}}</p>
                </div>
            </td>
        </tr>
    </tbody>
</table>
</body>
</html>