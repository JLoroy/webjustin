<html>
<head>
    <title>Flood Analysis</title>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <!--[if lte IE 8]><script src="assets/js/ie/html5shiv.js"></script><![endif]-->
    <link rel="stylesheet" href="stylesheets/flood/flood.css" />
    <!--[if lte IE 8]><link rel="stylesheet" href="assets/css/ie8.css" /><![endif]-->
    <!--[if lte IE 9]><link rel="stylesheet" href="assets/css/ie9.css" /><![endif]-->

    <script src="http://ajax.googleapis.com/ajax/libs/jquery/1/jquery.min.js"></script>
    <script src="http://ajax.googleapis.com/ajax/libs/jqueryui/1/jquery-ui.min.js"></script>
    <script src="/bootstrap/js/bootstrap.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.3.15/angular.js"></script>
    <script src="https://code.angularjs.org/1.3.15/i18n/angular-locale_fr-be.js"></script>
    <script src="javascripts/flood.js"></script>
    <script src="/jquery.js"></script>
    <script src="/bootstrap/js/bootstrap.js"></script>
    <link href="/bootstrap/css/bootstrap.css" rel="stylesheet" />
    <link href='https://fonts.googleapis.com/css?family=Lemon' rel='stylesheet' type='text/css'>
    <link href='https://fonts.googleapis.com/css?family=Montserrat' rel='stylesheet' type='text/css'>
</head>
    <body  ng-App="flood" ng-controller="floodController">
    <div id="header"><h1 class="title">Flood Big Data Analysis</h1></div>
    <div id="container">
        <div id="sidePane" class="col-md-2">
            <ul>
                <li><a href="#header" type="button" class="btn btn-lg">Up</a></li>
                <li><a href="#header" type="button" class="btn btn-lg">Up</a></li>
                <li><a href="#header" type="button" class="btn btn-lg">Up</a></li>
                <li><a href="#header" type="button" class="btn btn-lg">Up</a></li>
            </ul>
        </div>
        <div id="main" class="col-md-10">
            <div class="flooder col-md-12">
                <div class="fHeader col-md-12">
                    <div class="fImg col-md-3">
                        <img ng-src="{{current_flooder.photo_url}}"/>
                    </div>
                    <div class="fStatHeader col-md-9">
                        <div class="fStatRow col-md-12">
                            <h2 id="name" class="col-md-8">{{current_flooder.nom}}</h2>
                            <div id="stars class=col-md-4">STAR STAR STAR STAR STAR</div>
                        </div>
                        <div class="fStatRow col-md-12">
                            <div class="col-md-4">{{current_flooder.tot_mess}}</div>
                            <div class="col-md-4">{{current_flooder.tot_word}}</div>
                            <div class="col-md-4">{{current_flooder.tot_wdif}}</div>
                        </div>
                    </div>
                </div>
                <div class="fContainer col-md-12"></div>
            </div>{{stats}}
        </div>
    </div>
    <div id="footer"><a href="#header" type="button" class="btn btn-lg">Up</a></div>
    </body>
</html>